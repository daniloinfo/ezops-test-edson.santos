{
    "family": "chat-application", 
    "networkMode": "awsvpc", 
    "containerDefinitions": [
        {
            "name": "chat-application", 
            "image": "httpd:2.4", 
            "portMappings": [
                {
                    "containerPort": 3000, 
                    "hostPort": 3000, 
                    "protocol": "tcp"
                }
            ], 
            "essential": true 
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ], 
    "cpu": "256", 
    "memory": "512"
}